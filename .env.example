# Database Configuration (Docker uses port 5433 to avoid conflict with local PostgreSQL)
DATABASE_URL=postgresql://liwanag:liwanag@localhost:5433/liwanag

# FullEnrich API Configuration
FULLENRICH_API_KEY=your_fullenrich_api_key_here
FULLENRICH_WEBHOOK_URL=https://your-domain.com/api/webhooks/enrichment

# Set to 'true' to use mock enrichment (for demo), 'false' to use real API
# If not set, system will use real API when webhook URL is valid
USE_MOCK_ENRICHMENT=true

# Webhook Signing Secrets
NEWSLETTER_WEBHOOK_SECRET=your_newsletter_webhook_secret_here
FULLENRICH_WEBHOOK_SECRET=your_fullenrich_webhook_secret_here

# Application Configuration
NODE_ENV=development
PORT=3000
